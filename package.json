{
  "name": "honox-valtown-template",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "mprocs",
    "vite": "vite",
    "build": "vite build --mode client && vite build",
    "build:wintertc": "vite build --mode client -c vite.wintertc.config.ts && vite build -c vite.wintertc.config.ts",
    "build:deno": "vite build --mode client && vite build --mode deno",
    "preview": "node --run build && node dist/index.js",
    "preview:bun": "bun run build && bun dist/index.js",
    "preview:deno": "deno run build && deno serve -A dist/index.js",
    "test": "bun test"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "ajv": "^8.18.0",
    "hono": "^4.11.9",
    "honox": "^0.1.54",
    "json-schema-to-form": "^0.0.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@hono/vite-build": "^1.9.3",
    "@hono/vite-dev-server": "^0.23.0",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.2.3",
    "magic-string": "^0.30.21",
    "mprocs": "^0.8.3",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "imports": {
    "#app/*": [
      "./app/*"
    ]
  },
  "prettier": {
    "semi": false
  }
}
